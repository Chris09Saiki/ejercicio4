<p style="color: green"><%= notice %></p>

<% content_for :title, "Usuarios" %>

<h1>Usuarios</h1>

<div id="usuarios">
  <% @usuarios.each do |usuario| %>
    <%= render usuario %>
    <p>
      <%= link_to "Ver este usuario", usuario %>
    </p>
  <% end %>
</div>

<%= link_to "Nuevo Usuario", new_usuario_path, class: "new-resource-link" %> |
<%= link_to "Ir a articulos", articulos_path, class: "extra-links" %> 
